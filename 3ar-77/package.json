{
  "name": "webar-complete-app",
  "version": "1.0.0",
  "description": "Complete WebAR application with marker generation, 3D editing, AR verification and APK build",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000 --https",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000 --https",
    "android:add": "npx cap add android",
    "android:sync": "npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:build-real": "node scripts/build-apk.js",
    "serve": "node server.js",
    "install-deps": "npm install && npx cap add android"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/cli": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "aframe": "^1.4.0",
    "cors": "^2.8.5",
    "dat.gui": "^0.7.9",
    "express": "^4.21.2",
    "jszip": "^3.10.1",
    "multer": "^1.4.5-lts.1",
    "three": "^0.157.0"
  },
  "devDependencies": {
    "@types/three": "^0.157.0",
    "live-server": "^1.2.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.9"
  },
  "keywords": [
    "WebAR",
    "AR.js",
    "Three.js",
    "Capacitor",
    "Android",
    "3D",
    "Augmented Reality"
  ],
  "author": "WebAR Developer",
  "license": "MIT"
}
